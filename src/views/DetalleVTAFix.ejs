<script>
  $(document).ready(function () {
    $("#tableSearch").on("keyup", function () {
      var value = $(this).val().toLowerCase();
      $("#contenedorTable tr").filter(function () {
        $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
      });
    });
  });
  function convert(str) {
    var date = new Date(str), mnth = ("0" + (date.getMonth() + 1)).slice(-2), day = ("0" + date.getDate()).slice(-2);
    return [date.getFullYear(), mnth, day].join("-");
  }

  function preventBack() { window.history.forward(); }
  setTimeout("preventBack()", 0);
  window.onunload = function () { null };

  function genBoleta() {
    let timerInterval
    Swal.fire({
      title: 'Generando Boleta...',
      timer: 2000,
      timerProgressBar: true,
      onBeforeOpen: () => {
        Swal.showLoading()
        timerInterval = setInterval(() => {
          const content = Swal.getContent()
          if (content) {
            const b = content.querySelector('b')
            if (b) {
              b.textContent = Swal.getTimerLeft()
            }
          }
        }, 100)
      },
      onClose: () => {
        clearInterval(timerInterval)
      }
    }).then((result) => {
      /* Read more about handling dismissals below */
      if (result.dismiss === Swal.DismissReason.timer) {
        console.log('Message was closed by the timer')
      }
    })
    var doc = new jsPDF();

    var rut = "<%=sesion.userID%>"; //rut de la persona que hizo la compra
    var medioPago = "<%=salesdet.rows[0].mediopago%>"; //medio de pago
    var codVenta = "<%=salesdet.rows[0].codigov%>"
    var fechaa = convert("<%=salesdet.rows[0].fechaventa%>")

    doc.setFont('helvetica');
    doc.setFontType('bold');
    doc.setFontSize(24);
    doc.setLineWidth(1);
    doc.rect(120, 10, 80, 26);
    doc.text(123, 20, 'Boleta electrónica');
    doc.text(138, 30, 'SantaFeria');
    doc.setFontType('normal');
    doc.setFontSize(14);
    doc.text(20, 20, 'Rut del Vendedor: ' + rut);
    doc.text(20, 28, 'Fecha de venta: ' + fechaa);
    doc.text(20, 44, 'Código de la venta: ' + codVenta);
    var salesdetBody = [];
        <% for (var i = 0; i < salesdet.rows.length; i++) {%>
      salesdetBody.push(['<%= salesdet.rows[i].nombre%>', '<%= salesdet.rows[i].precio%>', '<%= salesdet.rows[i].cantproductos%>',
        (<%= salesdet.rows[i].precio %>*<%= salesdet.rows[i].cantproductos %>)]);
        <% } %>

      doc.autoTable({
        styles: { textColor: [0, 0, 0] },
        headStyles: { fillColor: [202, 202, 202] },
        head: [['Nombre', 'Precio', 'Cantidad', 'Monto']],

        body: salesdetBody,
        startY: 52,
        //style: fillColor:[52,58,64]	
      })
    var total = '<%= salesdet.rows[0].montototal%>';
    medioPago = 'Medio de pago: ' + medioPago;
    var totalFila = [medioPago, ' ', 'Total: ', total];
    doc.autoTable({
      styles: { textColor: [0, 0, 0] },
      headStyles: { fillColor: [202, 202, 202] },
      head: [totalFila],
    })
    var imgData = 'data:image/jpeg;base64,';
    doc.addImage(imgData, 'JPEG', 57, 220, 100, 45);
    var Footer = ['Santa Feria - Abarrotes y Mercadería para todos'];
    doc.autoTable({
      styles: { textColor: [0, 0, 0] },
      headStyles: { fillColor: [202, 202, 202] },
      head: [Footer],
      startY: 275,
    })
    doc.save('Boleta ' + rut + ' ' + fechaa + '.pdf');
  }

</script>

